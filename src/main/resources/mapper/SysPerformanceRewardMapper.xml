<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="xin.admin.mapper.earningsManagenment.SysPerformanceRewardMapper">
    <select id="selectPageWithUser" resultType="xin.admin.domain.earningsManagenment.SysPerformanceReward">
        SELECT spr.*, u.name, u.phone, u.myself_number
        FROM sys_performance_reward spr
        LEFT JOIN sys_user u ON spr.user_id = u.id
        <where>
            ${ew.sqlSegment}
        </where>
    </select>
</mapper>